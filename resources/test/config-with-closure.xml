<simulation>
	<cars default_strategy="traffic.algorithm.routing.LookaheadShortestPathRoutingAlgorithm">
		<car id="0" start="0" end="2" />
		<car id="1" start="4" end="7" />
		<car id="2" start="3" end="9" strategy="traffic.algorithm.routing.RandomRoutingAlgorithm" />
		<car id="3" start="8" end="4" strategy="traffic.algorithm.routing.RandomRoutingAlgorithm" />
		<car id="4" start="3" end="7" />
	</cars>

	<graph default_strategy="traffic.algorithm.delay.QuadraticTrafficDelayFunction" default_capacity="0">
		<node id="0" neighbors="4 7" />
		<node id="1" strategy="traffic.algorithm.LinearTrafficDelayFunction" neighbors="5" capacity ="0"/>
		<node id="2" strategy="traffic.algorithm.LinearTrafficDelayFunction" neighbors="4 7 9" />
		<node id="3" neighbors="1" />
		<node id="4" strategy="traffic.algorithm.LinearTrafficDelayFunction" neighbors="6 8" />
		<node id="5" strategy="traffic.algorithm.LinearTrafficDelayFunction" neighbors="0 2 9" />
		<node id="6" neighbors="5 8" capacity ="0"/>
		<node id="7" neighbors="0 1" />
		<node id="8" neighbors="3" />
		<node id="9" strategy="traffic.algorithm.LinearTrafficDelayFunction" neighbors="2" />
	</graph>
	
	<agents>
		<agent id="closure" class="traffic.agent.RoadClosureAgent" parameters="0.5:100" />
	</agents>
</simulation>