<simulation>
	<cars default_strategy="traffic.strategy.LookaheadShortestPathCarStrategy">
		<car id="0" start="0" end="2" />
		<car id="1" start="4" end="7" />
		<car id="2" start="3" end="9" strategy="traffic.strategy.RandomCarStrategy" />
		<car id="3" start="8" end="4" strategy="traffic.strategy.RandomCarStrategy" />
		<car id="4" start="3" end="7" />
	</cars>

	<graph default_strategy="traffic.strategy.QuadraticSpeedStrategy" default_capacity="5">
		<!-- node 27 doesn't exist and should be ignored as a neighbor-->
		<node id="0" neighbors="4 27" />
		<!-- node 55 does not exist, and since this node has no other neighbors it should be dropped -->
		<node id="1" strategy="traffic.strategy.LinearSpeedStrategy" neighbors="55" />
		<node id="2" strategy="traffic.strategy.LinearSpeedStrategy" neighbors="4 7 9" />
		<!-- node 3 will be dropped because is has no valid neighbor -->
		<node id="3" neighbors="1" />
		<node id="4" strategy="traffic.strategy.LinearSpeedStrategy" neighbors="6 8" />
		<node id="5" strategy="traffic.strategy.LinearSpeedStrategy" neighbors="" />
		<node id="6" neighbors="9 8" />
		<node id="7" neighbors="0 1" />
		<!-- node 8 will be dropped because it has no valid neighbor -->
		<node id="8" neighbors="3" />
		<node id="9" strategy="traffic.strategy.LinearSpeedStrategy" neighbors="2" />
	</graph>
</simulation>